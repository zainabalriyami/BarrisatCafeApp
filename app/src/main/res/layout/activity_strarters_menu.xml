<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StrartersMenu">


    <RelativeLayout
        android:id="@+id/StatersMenu"
        android:layout_width="230dp"
        android:layout_height="match_parent"
        android:background="@color/green" />

    <ScrollView
        android:id="@+id/SM"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rowTital"
                android:layout_width="222dp"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tvStartersMenu"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="20dp"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="@string/Starters"
                    android:textColor="@color/white"
                    android:textSize="40sp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rowstartmenu1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardStarters1"
                    android:layout_width="330dp"
                    android:layout_height="120dp"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp"
                    android:background="@color/white">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/start1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="80dp"
                            android:text="@string/Starters1"
                            android:textSize="23sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"
                            />



                        <RatingBar
                            android:id="@id/rating_starters1"
                            style="@style/Widget.AppCompat.RatingBar.Indicator"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/start1"
                            android:layout_marginTop="1dp"
                            android:layout_marginLeft="80dp"
                            android:isIndicator="false"
                            android:progressTint="@color/Yellow" />

                        <TextView
                            android:id="@+id/tvpriceS11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/rating_starters1"
                            android:layout_marginLeft="80dp"
                            android:text="@string/Dprice"
                            android:textSize="14sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"/>

                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <ImageView
                    android:id="@+id/imagstart1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginBottom="4dp"
                    android:elevation="2dp"
                    android:src="@drawable/dycheckin" />
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabstarter1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="6dp"
                    app:tint="@color/green"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/baseline_add_24" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rowstartmenu2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardstrat2"
                    android:layout_width="330dp"
                    android:layout_height="120dp"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp"
                    android:background="@color/white">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/start2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="80dp"
                            android:text="@string/Starters2"
                            android:textSize="23sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"
                            />

                        <RatingBar
                            android:id="@id/ratingstart2"
                            style="@style/Widget.AppCompat.RatingBar.Indicator"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/start2"
                            android:layout_marginTop="1dp"
                            android:layout_marginLeft="80dp"
                            android:isIndicator="false"
                            android:progressTint="@color/Yellow" />

                        <TextView
                            android:id="@+id/tvpriceS2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/ratingstart2"
                            android:layout_marginLeft="80dp"
                            android:text="@string/shprice"
                            android:textSize="14sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"/>

                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <ImageView
                    android:id="@+id/imagstart2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginBottom="4dp"
                    android:elevation="2dp"
                    android:src="@drawable/dyshrimp" />
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabstar2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="6dp"
                    app:tint="@color/green"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/baseline_add_24" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rowstartmenu3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardstart3"
                    android:layout_width="330dp"
                    android:layout_height="120dp"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp"
                    android:background="@color/white">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/start3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="80dp"
                            android:text="@string/Starters3"
                            android:textSize="21sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"
                            />


                        <RatingBar
                            android:id="@id/rating_start3"
                            style="@style/Widget.AppCompat.RatingBar.Indicator"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/start3"
                            android:layout_marginTop="1dp"
                            android:layout_marginLeft="80dp"
                            android:isIndicator="false"
                            android:progressTint="@color/Yellow" />

                        <TextView
                            android:id="@+id/tvprices1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/rating_start3"
                            android:layout_marginLeft="80dp"
                            android:text="@string/wingsprice"
                            android:textSize="14sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"/>

                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <ImageView
                    android:id="@+id/imagstart3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/checkinw"
                    android:elevation="2dp"
                    android:layout_margin="4dp"
                    />
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabstart3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="6dp"
                    app:tint="@color/green"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/baseline_add_24" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rowstartmenu4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardstart4"
                    android:layout_width="330dp"
                    android:layout_height="120dp"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp"
                    android:background="@color/white">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/start4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="80dp"
                            android:text="@string/Starters4"
                            android:textSize="21sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"
                            />

                        <RatingBar
                            android:id="@id/ratingstart4"
                            style="@style/Widget.AppCompat.RatingBar.Indicator"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/start4"
                            android:layout_marginTop="1dp"
                            android:layout_marginLeft="80dp"
                            android:isIndicator="false"
                            android:progressTint="@color/Yellow" />

                        <TextView
                            android:id="@+id/tvprices4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/ratingstart4"
                            android:layout_marginLeft="80dp"
                            android:text="@string/potatoprice"
                            android:textSize="14sp"
                            android:textColor="@color/green"
                            android:fontFamily="@font/poppins_semibold"/>

                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <ImageView
                    android:id="@+id/imagstart4"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginBottom="4dp"
                    android:elevation="2dp"
                    android:src="@drawable/potato" />
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabstart4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="6dp"
                    app:tint="@color/green"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/baseline_add_24" />
            </RelativeLayout>


        </LinearLayout>

    </ScrollView>

</FrameLayout>